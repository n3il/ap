# Supabase Edge Functions Environment Variables
# Copy this file to .env and fill in your actual values

# Hyperliquid API
# Documentation: https://hyperliquid.xyz/docs
HYPERLIQUID_API_URL=https://api.hyperliquid.xyz

# Optional: Hyperliquid private key for actual trade execution
# WARNING: Never commit this to version control!
# HYPERLIQUID_PRIVATE_KEY=your_private_key_here

# These are automatically provided by Supabase
# SUPABASE_URL=your_supabase_project_url
# SUPABASE_ANON_KEY=your_supabase_anon_key
# SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Provider API Keys
GOOGLE_GEMINI_API_KEY=api_key
DEEPSEEK_API_KEY=api_key
ANTHROPIC_API_KEY=api_key
OPENAI_API_KEY=api_key
