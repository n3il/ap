{
  "cli": {
    "version": ">= 14.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT_EXT": ".development"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "env": {
        "APP_VARIANT_EXT": ".preview"
      }
    },
    "production": {
      "channel": "production",
      "ios": {
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}